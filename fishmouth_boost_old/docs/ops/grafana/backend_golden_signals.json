{
  "dashboard": {
    "title": "Fish Mouth - Backend Golden Signals",
    "panels": [
      {"type": "graph", "title": "Requests/sec", "targets": [{"expr": "http_requests_total"}]},
      {"type": "graph", "title": "Latency p95", "targets": [{"expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))"}]},
      {"type": "graph", "title": "5xx errors", "targets": [{"expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m]))"}]}
    ]
  }
}
